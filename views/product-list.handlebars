<div class="container blog">
    <div class="row">
      <div class="col-md-2 col-sm-2">
        <a class="btn btn-light" href="/product">Add New Product</a>
      </div>
      <div class="col-md-2 col-sm-2">
        <a class="btn btn-light" href="/members">Dashboard</a>
      </div>
    </div>
    <div class="row" style="margin-top: 20px;">
      <div class="col-md-12 blog-container">
        {{#each products}}

<div>{{name}} 
     {{id}}
     {{price}}
     {{description}}</div>
        {{/each}}
      </div>
    </div>
  </div>
